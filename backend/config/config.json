{
  "development": {
    "use_env_variable": "DATABASE_URL_NON_POOLING",
    "dialect": "mysql"
  },
  "production": {
    "use_env_variable": "POSTGRES_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}